{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AppSettings": {
    "BestStoryIdsCacheName": "BestStoryIds",
    "CacheExpirationTimeMinutes": 0.5,
    "DateFormat": "yyyy-MM-ddTHH:mm:sszzz",
    "HackerNewsApiUrl": "https://hacker-news.firebaseio.com/v0/",
    "GetBestStoryIdsAsyncPath": "beststories.json",
    "GetStoryDetailsAsyncPath": "item/id/.json"
  }
}